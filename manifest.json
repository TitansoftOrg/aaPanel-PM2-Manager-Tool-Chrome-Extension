{
  "name": "aaPanel PM2 Manager Tool",
  "description": "aaPanel PM2 Manager için oto refresh özelliği sağlar, clear işlemindeki captchayı yanıtlar.",
  "version": "1.0",
  "manifest_version": 3,
   "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.6.4.min.js",
        "content.js"
      ]
    }
  ]
}
